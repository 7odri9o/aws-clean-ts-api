{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",
  "compilerOptions": {
    "outDir": "dist",
    "module":"commonjs",
    "target": "es2020",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts"
  ]
}
